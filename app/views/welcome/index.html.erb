<%@country=Country.all%>
<div class="row row-cols-1 row-cols-md-2 g-4">
<%@country.each do |country|%>

  <div class="col">
    <div class="card">
      <img src="..." class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title"><%=link_to country.name, "/hey"%></h5>
        <p class="card-text"></p>
      </div>
    </div>
  </div>

<%end%>
</div>

