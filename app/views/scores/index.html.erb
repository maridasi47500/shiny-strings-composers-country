<p style="color: green"><%= notice %></p>

<% content_for :title, "Scores" %>

<h1>Scores</h1>

<div id="scores">
  <% @scores.each do |score| %>
    <%= render score %>
    <p>
      <%= link_to "Show this score", score %>
    </p>
  <% end %>
</div>

<%current_user.composers.each do |h|%>
<%= link_to "New score by #{h.name}", new_score_path(composer_id: h.id) %>
<%end%>
